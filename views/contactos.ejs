<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Su contacto del formulario</title>
</head>
<body>
    <table>
        <thead>
          <tr>
            <th>Nombre</th>
            <th>Email</th>
            <th>Comentario</th>
            <th>Fecha-Hora</th>
            <th>IP</th>
            <th>pais</th>
            <th>country</th>
          </tr>
        </thead>
        <tbody>
          <% registros.forEach(function(registro) { %>
          <tr>
            <th><%= registro.name%></th>
            <td><%= registro.email%></td>
            <td><%= registro.comentario%></td>
            <td><%= registro.fecha%></td>
            <td><%= registro.ip%></td>
            <td><%= registro.pais%</td>
            <td><%= registro.country%></td>
          </tr>
          <% } %>
        </tbody>
      </table>
</html>